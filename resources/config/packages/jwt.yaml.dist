# Configurazione per la login
jwt:
  secret_key: '%env(JWT_PASSPHRASE)%'
  token_ttl: 3600  # Scadenza del token in secondi (1 ora)  # Default 3600
  refresh_token_ttl: 2592000  # Scadenza del token di refresh in secondi  # Default 7 giorni (in secondi) # Attuale: 30 giorni
  algorithm: 'HS256' # Default HS256
